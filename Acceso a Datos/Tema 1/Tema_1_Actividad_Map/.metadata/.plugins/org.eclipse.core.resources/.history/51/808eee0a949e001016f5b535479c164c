package actividad1;

import java.util.Hashtable;
import java.util.Map;

public class Taller {
	private Map <Matricula, Vehiculo> vehiculos;

	public Taller() {
		
		this.vehiculos = new Hashtable<>();
	}
	
	public void aÃ±adirVehiculo(Vehiculo v) {
		
		/*put(clave, valor) -> devuelve null
		 * 
		 */
		vehiculos.put(v.getMatricula(), v);
	}
}
